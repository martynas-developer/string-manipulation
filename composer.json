{
    "require": {
        "php": "8.2.*",
        "ext-mbstring": "*",
        "ext-intl" : "*",
        "symfony/dependency-injection": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
