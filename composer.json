{
    "require": {
        "php": "8.2.*",
        "symfony/dependency-injection": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "*",
        "phpstan/phpstan": "^1.10"
    }
}
